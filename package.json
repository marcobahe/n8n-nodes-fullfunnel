{
  "name": "n8n-nodes-fullfunnel",
  "version": "1.17.0",
  "description": "n8n nodes for FullFunnel (GoHighLevel) integration - Contacts, Tags and Opportunities",
  "keywords": [
    "n8n-community-node-package",
    "n8n",
    "fullfunnel",
    "gohighlevel",
    "highlevel",
    "crm",
    "marketing",
    "automation"
  ],
  "author": {
    "name": "Marco Bahe",
    "email": "marcobahe@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/marcobahe/n8n-nodes-fullfunnel"
  },
  "license": "MIT",
  "homepage": "https://fullfunnel.io",
  "engines": {
    "node": ">=18.10",
    "pnpm": ">=9.1"
  },
  "packageManager": "pnpm@9.1.4",
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/FullFunnelApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/FullFunnel/FullFunnel.node.js",
      "dist/nodes/FullFunnel/FullFunnelTags.node.js",
      "dist/nodes/FullFunnel/FullFunnelOpportunities.node.js"
    ]
  },
  "files": [
    "dist",
    "icons"
  ],
  "main": "index.js",
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "build": "tsc",
    "dev": "tsc --watch",
    "format": "prettier nodes credentials --write",
    "lint": "eslint nodes credentials package.json",
    "lintfix": "eslint nodes credentials package.json --fix",
    "prepublishOnly": "pnpm build && pnpm run lint -c .eslintrc.prepublish.json nodes credentials package.json"
  },
  "devDependencies": {
    "@typescript-eslint/parser": "^7.15.0",
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.5",
    "@typescript-eslint/eslint-plugin": "^7.13.1",
    "eslint": "^8.57.1",
    "eslint-plugin-n8n-nodes-base": "^1.16.1",
    "gulp": "^4.0.2",
    "n8n-workflow": "*",
    "prettier": "^3.3.2",
    "typescript": "^5.5.3"
  },
  "peerDependencies": {
    "n8n-workflow": "*"
  }
}
